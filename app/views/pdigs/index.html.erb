<h1>Listing pdigs</h1>

<table>
  <tr>
    <th>User</th>
    <th>Photo</th>
  </tr>

<% @pdigs.each do |pdig| %>
  <tr>
    <td><%=h pdig.user_id %></td>
    <td><%=h pdig.photo_id %></td>
    <td><%= link_to 'Show', pdig %></td>
    <td><%= link_to 'Edit', edit_pdig_path(pdig) %></td>
    <td><%= link_to 'Destroy', pdig, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New pdig', new_pdig_path %>