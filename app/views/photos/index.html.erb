<h1>Listing photos</h1>

<table>
  <tr>
    <th>Digs count</th>
    <th>Album</th>
    <th>Cover</th>
    <th>Notation</th>
    <th>Parent</th>
    <th>Content type</th>
    <th>File name</th>
    <th>Thumbnail</th>
    <th>Size</th>
    <th>Width</th>
    <th>Height</th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%=h photo.digs_count %></td>
    <td><%=h photo.album_id %></td>
    <td><%=h photo.cover %></td>
    <td><%=h photo.notation %></td>
    <td><%=h photo.parent_id %></td>
    <td><%=h photo.content_type %></td>
    <td><%=h photo.file_name %></td>
    <td><%=h photo.thumbnail %></td>
    <td><%=h photo.size %></td>
    <td><%=h photo.width %></td>
    <td><%=h photo.height %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New photo', new_photo_path %>