<h1>Editing vdig</h1>

<% form_for(@vdig) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :video_id %><br />
    <%= f.text_field :video_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @vdig %> |
<%= link_to 'Back', vdigs_path %>